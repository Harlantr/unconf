{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-shadow": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/forbid-prop-types": "none",
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "import/prefer-default-export": "none",
    "react/prop-types": "none",
    "jsx-a11y/label-has-for": "none",
    "import/no-named-as-default": "none",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
